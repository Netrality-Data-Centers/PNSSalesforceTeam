    <decisions>
        <name>Counter_is_Less_or_Equal_than_Fiber_Pairs_Commited</name>
        <label>Counter is Less or Equal than Fiber Pairs Commited?</label>
        <locationX>182</locationX>
        <locationY>350</locationY>
        <defaultConnector>
            <targetReference>Set_FiberPair_NotCommited</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Yes</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Counter</leftValueReference>
                <operator>LessThanOrEqualTo</operator>
                <rightValue>
                    <elementReference>FiberPairsCommited</elementReference>
                </rightValue>
            </conditions>
            <connector>
